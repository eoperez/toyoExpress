{
  "name": "toyo-express",
  "version": "0.0.0",
  "main": "./dist/backEnd/main.js",
  "description": "Simple DMS for toyoEpress",
  "scripts": {
    "ng": "ng",
    "rebuildElectron": "electron-rebuild -f -w sqlite3",
    "compileBackEnd": "tsc -p tsconfigElectron.json",
    "compileDbScripts": "tsc -p tsconfigKnex.json",
    "start": "npm run compileDbScripts && npm run compileBackEnd && ng build --base-href ./ && electron .",
    "startClient": "ng serve",
    "build": "npm run compileBackEnd && ng build --base-href ./ ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eoperez/toyoExpress.git"
  },
  "keywords": [
    "Electron",
    "Angular",
    "Foundation",
    "SQLite",
    "Knex"
  ],
  "author": "Edwin Perez",
  "bugs": {
    "url": "https://github.com/eoperez/toyoExpress/issues"
  },
  "homepage": "https://github.com/eoperez/toyoExpress#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "foundation-sites": "^6.5.0",
    "jquery": "^3.3.1",
    "knex": "^0.15.2",
    "motion-ui": "^2.0.3",
    "ngx-cookie-service": "^2.0.0",
    "ngx-electron": "^2.0.0",
    "rxjs": "~6.3.3",
    "sqlite3": "^4.0.4",
    "what-input": "^5.1.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/foundation-sites": "^6.4.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.22",
    "@types/knex": "^0.15.0",
    "@types/node": "~8.9.4",
    "@types/sqlite3": "^3.1.3",
    "codelyzer": "~4.5.0",
    "electron": "^3.0.9",
    "electron-rebuild": "^1.8.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
